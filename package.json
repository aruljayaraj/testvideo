    "@types/react-image-crop": "^8.1.2",
    "@types/react-redux": "^7.1.9",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.1",
    "capacitor-voice-recorder": "^2.1.0",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-media-capture": "^3.0.3",
    "cordova-plugin-nativeaudio": "^3.0.9",
    "date-fns": "^2.15.0",
    "env-cmd": "^10.1.0",
    "font-awesome": "^4.7.0",
    "immer": "^8.0.1",
